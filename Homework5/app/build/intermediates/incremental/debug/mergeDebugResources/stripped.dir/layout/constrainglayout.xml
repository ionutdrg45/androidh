<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelinetop"
        app:layout_constraintGuide_begin="@dimen/md2"
        android:orientation="horizontal"/>

    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guidelinebottom"
        app:layout_constraintGuide_percent="0.97"
        android:orientation="horizontal"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vertical50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.5"
        android:orientation="vertical"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/horizontal50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.5"
        android:orientation="horizontal"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vertical25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent="0.25"
        android:orientation="vertical"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/horizontal25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.25" />

    <TextView
        android:id="@+id/textview1"
        app:layout_constraintTop_toBottomOf="@id/guidelinetop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="@dimen/tlg"/>

    <TextView
        android:id="@+id/textview2"
        app:layout_constraintTop_toBottomOf="@id/textview1"
        app:layout_constraintEnd_toStartOf="@id/vertical50"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="titleeeeeeee2"
        android:textSize="@dimen/tsm2"
        android:layout_marginTop="@dimen/sm3"/>

    <TextView
        android:id="@+id/textview3"
        app:layout_constraintTop_toBottomOf="@id/textview2"
        app:layout_constraintStart_toStartOf="@id/vertical50"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="eeeee ddddd"
        android:textSize="@dimen/tsm2"/>

    <ImageView
        android:id="@+id/imageview1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/image"
        app:layout_constraintTop_toBottomOf="@id/textview3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/vertical25"/>

    <CheckBox
        android:id="@+id/checkbox1"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="@id/vertical25"
        app:layout_constraintEnd_toStartOf="@id/vertical50"
        android:text="CheckBox"
        android:textSize="@dimen/tsm"
        android:checked="true"
        app:layout_constraintTop_toBottomOf="@id/textview3"/>

    <TextView
        android:id="@+id/textview4"
        app:layout_constraintTop_toBottomOf="@id/imageview1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/vertical25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CC"
        android:textSize="@dimen/tsm2"/>

    <TextView
        android:id="@+id/textview5"
        app:layout_constraintTop_toBottomOf="@id/imageview1"
        app:layout_constraintStart_toStartOf="@id/vertical25"
        app:layout_constraintEnd_toStartOf="@id/vertical50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FF"
        android:textSize="@dimen/tsm2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/horizontal25"
        app:layout_constraintBottom_toTopOf="@id/horizontal50"
        app:layout_constraintStart_toEndOf="@id/vertical50"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Button" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/horizontal50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/vertical50"
        android:text="Button" />

    <TextView
        android:id="@+id/textview6"
        app:layout_constraintTop_toBottomOf="@id/horizontal50"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:textSize="@dimen/tsm2"
        android:paddingStart="@dimen/sm1"
        android:paddingEnd="@dimen/sm1" />

    <TextView
        android:id="@+id/textview7"
        app:layout_constraintTop_toBottomOf="@id/horizontal50"
        app:layout_constraintEnd_toStartOf="@id/vertical50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:textSize="@dimen/tsm2"
        android:paddingStart="@dimen/sm1"
        android:paddingEnd="@dimen/sm1"/>

    <TextView
        android:id="@+id/textview8"
        app:layout_constraintTop_toBottomOf="@id/horizontal50"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:textSize="@dimen/tsm2"
        android:paddingStart="@dimen/sm1"
        android:paddingEnd="@dimen/sm1"/>

    <TextView
        android:id="@+id/textview9"
        app:layout_constraintTop_toBottomOf="@id/textview7"
        app:layout_constraintEnd_toStartOf="@id/vertical50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:textSize="@dimen/tsm2"
        android:paddingStart="@dimen/sm1"
        android:paddingEnd="@dimen/sm1"/>

    <TextView
        android:id="@+id/textview10"
        app:layout_constraintTop_toBottomOf="@id/textview7"
        app:layout_constraintStart_toEndOf="@id/vertical50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:textSize="@dimen/tsm2"
        android:paddingStart="@dimen/sm1"
        android:paddingEnd="@dimen/sm1"/>

    <ImageView
        android:id="@+id/imageview2"
        android:layout_width="200dp"
        android:layout_height="140dp"
        android:src="@drawable/image"
        app:layout_constraintTop_toBottomOf="@id/horizontal50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/guidelinebottom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/vertical50"
        android:text="Button" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/guidelinebottom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/vertical50"
        android:text="Button" />

    <Button
        android:layout_width="@dimen/lg3"
        android:layout_height="@dimen/lg1"
        android:paddingStart="@dimen/sm1"
        android:paddingEnd="@dimen/sm1"
        app:layout_constraintBottom_toTopOf="@id/button1"
        app:layout_constraintEnd_toStartOf="@id/vertical25"
        app:layout_constraintStart_toStartOf="parent"
        android:text="Button" />

</androidx.constraintlayout.widget.ConstraintLayout>